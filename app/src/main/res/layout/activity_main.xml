<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_content"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context=".MainActivity">

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

    </androidx.viewpager.widget.ViewPager>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end|bottom"
        android:layout_margin="@dimen/fab_margin"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:backgroundTint="@android:color/holo_blue_dark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ic_check_circle_black_24dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:layout_margin="0dp"
        android:paddingTop="0dp"
        android:scaleType="center"
        android:src="@drawable/bg5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/candidateName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-smallcaps"
        android:paddingBottom="12dp"
        android:text="Archiel Nouf Espino"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="179dp"
        android:layout_height="176dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="8dp"
        android:scaleType="fitXY"
        android:src="@drawable/bg3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

    <ImageView
        android:id="@+id/starPic"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:cropToPadding="true"
        android:padding="8dp"
        android:src="@drawable/contestant1"
        app:layout_constraintBottom_toBottomOf="@+id/imageView8"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="@+id/imageView8" />

    <ImageView
        android:id="@+id/takipan"
        android:layout_width="0dp"
        android:layout_height="332dp"
        android:layout_marginTop="5dp"
        android:paddingLeft="8dp"
        android:paddingRight="8dp"
        android:scaleType="fitXY"
        android:src="@drawable/bg4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/starName" />

    <com.warkiz.widget.IndicatorSeekBar
        android:id="@+id/seekbar3"
        android:layout_width="0dp"
        android:layout_height="37dp"

        android:layout_marginStart="8dp"

        android:layout_marginTop="4dp"

        android:layout_marginEnd="8dp"
        android:duplicateParentState="false"

        android:fadeScrollbars="true"
        android:fadingEdge="horizontal"
        android:filterTouchesWhenObscured="true"
        android:keepScreenOn="true"
        app:chainUseRtl="false"
        app:isb_indicator_color="@android:color/holo_blue_dark"
        app:isb_indicator_text_color="#ffffff"
        app:isb_indicator_text_size="18sp"
        app:isb_max="30"
        app:isb_min="1.0"

        app:isb_progress="1"
        app:isb_r2l="false"
        app:isb_seek_smoothly="true"
        app:isb_show_indicator="circular_bubble"
        app:isb_show_thumb_text="true"
        app:isb_show_tick_marks_type="none"
        app:isb_show_tick_texts="true"
        app:isb_thumb_adjust_auto="true"
        app:isb_thumb_color="@color/head"
        app:isb_thumb_drawable="@drawable/thumbmic"
        app:isb_thumb_size="25dp"
        app:isb_tick_marks_ends_hide="true"
        app:isb_tick_marks_size="13dp"
        app:isb_tick_marks_swept_hide="true"
        app:isb_tick_texts_color="@color/common_google_signin_btn_text_dark_pressed"
        app:isb_tick_texts_size="7sp"
        app:isb_ticks_count="30"
        app:isb_track_background_color="@android:color/darker_gray"
        app:isb_track_background_size="23dp"
        app:isb_track_progress_color="@android:color/holo_blue_bright"
        app:isb_track_progress_size="4dp"
        app:isb_track_rounded_corners="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/starName"
        android:layout_width="0dp"
        android:layout_height="37dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="cursive"
        android:text="Camila Cabello"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Appearance and Stage Impact: (30%)"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/seekbar2"
        app:layout_constraintTop_toBottomOf="@+id/seekbar" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Lip Synchronization: (20%)"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/seekbar"
        app:layout_constraintTop_toTopOf="@+id/takipan" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Choreography: (30%)"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/seekbar3"
        app:layout_constraintTop_toBottomOf="@+id/seekbar2" />

    <com.warkiz.widget.IndicatorSeekBar
        android:id="@+id/seekbar2"
        android:layout_width="0dp"
        android:layout_height="50dp"

        android:layout_marginStart="8dp"

        android:layout_marginTop="4dp"

        android:layout_marginEnd="8dp"
        android:duplicateParentState="false"
        android:fadeScrollbars="true"
        android:fadingEdge="horizontal"
        android:filterTouchesWhenObscured="true"
        android:keepScreenOn="true"
        app:chainUseRtl="false"
        app:isb_indicator_color="@android:color/holo_blue_dark"
        app:isb_indicator_text_color="#ffffff"
        app:isb_indicator_text_size="18sp"
        app:isb_max="30"
        app:isb_min="1.0"

        app:isb_progress="1"
        app:isb_r2l="false"
        app:isb_seek_smoothly="true"
        app:isb_show_indicator="circular_bubble"
        app:isb_show_thumb_text="true"
        app:isb_show_tick_marks_type="none"
        app:isb_show_tick_texts="false"
        app:isb_thumb_adjust_auto="true"
        app:isb_thumb_color="@color/head"
        app:isb_thumb_drawable="@drawable/thumbmic"
        app:isb_thumb_size="25dp"
        app:isb_tick_marks_ends_hide="true"
        app:isb_tick_marks_size="13dp"
        app:isb_tick_marks_swept_hide="true"
        app:isb_tick_texts_color="@color/common_google_signin_btn_text_dark_pressed"
        app:isb_tick_texts_size="7sp"
        app:isb_ticks_count="30"
        app:isb_track_background_color="@android:color/darker_gray"
        app:isb_track_background_size="23dp"
        app:isb_track_progress_color="@android:color/holo_blue_bright"
        app:isb_track_progress_size="4dp"
        app:isb_track_rounded_corners="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <com.warkiz.widget.IndicatorSeekBar
        android:id="@+id/seekbar"
        android:layout_width="0dp"
        android:layout_height="50dp"

        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:duplicateParentState="false"
        android:fadeScrollbars="true"
        android:fadingEdge="horizontal"
        android:filterTouchesWhenObscured="true"
        android:keepScreenOn="true"

        app:chainUseRtl="true"
        app:isb_indicator_color="@android:color/holo_blue_dark"
        app:isb_indicator_text_color="#ffffff"
        app:isb_indicator_text_size="18sp"
        app:isb_max="20"
        app:isb_min="1.0"
        app:isb_progress="1"
        app:isb_r2l="false"
        app:isb_seek_smoothly="true"
        app:isb_show_indicator="circular_bubble"
        app:isb_show_thumb_text="true"
        app:isb_show_tick_marks_type="none"
        app:isb_show_tick_texts="true"
        app:isb_thumb_adjust_auto="true"
        app:isb_thumb_color="@color/head"
        app:isb_thumb_drawable="@drawable/thumbmic"
        app:isb_thumb_size="25dp"
        app:isb_tick_marks_ends_hide="true"
        app:isb_tick_marks_size="13dp"
        app:isb_tick_marks_swept_hide="true"
        app:isb_tick_texts_color="@color/common_google_signin_btn_text_dark_pressed"
        app:isb_tick_texts_size="9sp"
        app:isb_ticks_count="20"
        app:isb_track_background_color="@android:color/darker_gray"
        app:isb_track_background_size="23dp"
        app:isb_track_progress_color="@android:color/holo_blue_bright"
        app:isb_track_progress_size="4dp"
        app:isb_track_rounded_corners="true"
        app:layout_constraintEnd_toEndOf="@+id/seekbar2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Entertainment-wise: (20%)"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/seekbar4"
        app:layout_constraintTop_toBottomOf="@+id/seekbar3" />

    <com.warkiz.widget.IndicatorSeekBar
        android:id="@+id/seekbar4"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"

        android:layout_marginEnd="8dp"
        android:duplicateParentState="false"

        android:fadeScrollbars="true"

        android:fadingEdge="horizontal"
        android:filterTouchesWhenObscured="true"

        android:keepScreenOn="true"

        app:chainUseRtl="false"
        app:isb_indicator_color="@android:color/holo_blue_dark"
        app:isb_indicator_text_color="#ffffff"
        app:isb_indicator_text_size="18sp"
        app:isb_max="20"
        app:isb_min="1.0"

        app:isb_progress="1"
        app:isb_r2l="false"
        app:isb_seek_smoothly="true"
        app:isb_show_indicator="circular_bubble"
        app:isb_show_thumb_text="true"
        app:isb_show_tick_marks_type="none"
        app:isb_show_tick_texts="true"
        app:isb_thumb_adjust_auto="true"
        app:isb_thumb_color="@color/head"
        app:isb_thumb_drawable="@drawable/thumbmic"
        app:isb_thumb_size="25dp"
        app:isb_tick_marks_ends_hide="true"
        app:isb_tick_marks_size="13dp"
        app:isb_tick_marks_swept_hide="true"
        app:isb_tick_texts_color="@color/common_google_signin_btn_text_dark_pressed"
        app:isb_tick_texts_size="9sp"
        app:isb_ticks_count="20"
        app:isb_track_background_color="@android:color/darker_gray"
        app:isb_track_background_size="23dp"
        app:isb_track_progress_color="@android:color/holo_blue_bright"
        app:isb_track_progress_size="4dp"
        app:isb_track_rounded_corners="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.413"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <ImageView
        android:id="@+id/done"
        android:layout_width="329dp"
        android:layout_height="280dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/takipan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.453"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/takipan"
        app:layout_constraintVertical_bias="0.381"
        app:srcCompat="@drawable/done" />

</androidx.constraintlayout.widget.ConstraintLayout>